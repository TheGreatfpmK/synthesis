    s : [0..4] init 0;


    [] s=0 & x=0 -> 3/4 : (s'=3) + 1/4 : (s'=1);
    [] s=0 & x=1 -> (s'=4);

    [] s=1 & x=0 -> 3/4 : (s'=3) + 1/4 : (s'=4);
    [] s=1 & x=1 -> 1/10 : (s'=3) + 1/10 : (s'=4) + 8/10 : (s'=2);

